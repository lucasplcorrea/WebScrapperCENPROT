{
    "manifest_version": 3,
    "name": "Injetor de Script",
    "version": "2.0",
    "description": "Injeta script na p√°gina",
    "permissions": [
        "activeTab",
        "storage",
        "scripting"
    ],
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://cartorio.cenprotnacional.org.br/home/pedidos/certidao*"],
            "js": ["content.js"]
        }
    ]
}
